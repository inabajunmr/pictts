{
    "name": "pictts",
    "version": "0.0.2",
    "description": "PICT clone by TypeScript",
    "repository": {
        "type": "git",
        "url": "https://github.com/inabajunmr/pictts"
    },
    "bugs": {
        "url": "https://github.com/inabajunmr/pictts/issues"
    },
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest",
        "fix": "run-s fix:prettier fix:eslint",
        "fix:eslint": "eslint src --ext .ts --fix",
        "lint": "run-p lint:prettier lint:eslint",
        "lint:eslint": "eslint src --ext .ts",
        "fix:prettier": "prettier --write src",
        "lint:prettier": "prettier --check src",
        "build": "tsc",
        "prepublishOnly": "npm run build"
    },
    "keywords": ["PICT", "Testing"],
    "author": "inabajunmr",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^26.0.19",
        "@typescript-eslint/eslint-plugin": "^4.11.0",
        "@typescript-eslint/parser": "^4.11.0",
        "eslint": "^7.16.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-jest": "^24.1.3",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "jest": {
        "testEnvironment": "node"
    }
}
